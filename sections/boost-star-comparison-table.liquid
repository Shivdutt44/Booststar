{% capture css %}

<style>
  {% if section.settings.override_fonts %}
  {{ section.settings.text_font | font_face }}
  {% endif %}

  #bse-cota-{{ section.id }}.bse-cota-wrap div,
  #bse-cota-{{ section.id }}.bse-cota-wrap p,
  #bse-cota-{{ section.id }}.bse-cota-wrap th,
  #bse-cota-{{ section.id }}.bse-cota-wrap td,
  #bse-cota-{{ section.id }}.bse-cota-wrap a {
    font-size: {{ section.settings.text_size }}px;
    {% if section.settings.override_fonts %}
    font-family: {{ section.settings.text_font.family }}, {{ section.settings.text_font.fallback_families }};
    font-weight: {{ section.settings.text_font.weight }};
    {% endif %}
    {% if section.settings.text_line_height != 'inherit' %}line-height: {{ section.settings.text_line_height }};{% endif %}
  }

  @media only screen and (max-width: 800px) {
    #bse-cota-{{ section.id }}.bse-cota-wrap div,
    #bse-cota-{{ section.id }}.bse-cota-wrap p,
    #bse-cota-{{ section.id }}.bse-cota-wrap th,
    #bse-cota-{{ section.id }}.bse-cota-wrap td,
    #bse-cota-{{ section.id }}.bse-cota-wrap a {
      font-size: {{ section.settings.mobile_text_size }}px;
    }
  }

  #bse-cota-{{ section.id }}.bse-cota-wrap {
    background-image: {{section.settings.background_color}};
    width: 100%;
  }

  #bse-cota-{{ section.id }} .sp-comp {
    position: relative;
    margin: 0 auto;
    max-width: {{ section.settings.max_width }}px;
    padding: {{ section.settings.padding_top }}px {{ section.settings.padding_right }}px {{ section.settings.padding_top}}px {{ section.settings.padding_right }}px;
  }

  #bse-cota-{{ section.id }} .sp-comp h2 {
    text-align: center;
    color: {{ section.settings.text_color }};
    font-weight: bold;
    max-width: 1000px;
    margin: 0 auto 25px auto;
    padding: 0;
  }

  .sp-comp .bse-cota-grid {
    position: relative;
    margin: 0 auto;
    display: block;
    max-width: 1000px;
  }

  .bse-cota-row0, .bse-cota-row {
    display: grid;
    grid-template-columns: 3fr 150px 150px;
    border-bottom: 1px solid {{ section.settings.border_color }};
    gap: 0;
  }

  .bse-cota-row0 *, .bse-cota-row * {
    color: {{ section.settings.text_color }};
  }

  @media only screen and (max-width: 800px) {
    .bse-cota-row0, .bse-cota-row {
      display: grid;
      grid-template-columns: 3fr 100px 100px;
      border-bottom: 1px solid {{ section.settings.border_color }};
      gap: 0;
    }
  }

  .bse-cota-row0>div, .bse-cota-row>div {
    padding: {{ section.settings.cell_padding }}px;
  }

  .bse-cota-row0>div:nth-child(1), .bse-cota-row>div:nth-child(1) {
    text-align: left;
  }

  .bse-cota-row0>div:nth-child(2) {
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    font-weight: bold !important;
  }

  .bse-cota-row0>div:nth-child(2), .bse-cota-row>div:nth-child(2) {
    text-align: center;
    background-color: {{ section.settings.highlight_background }};
  }

  .bse-cota-row0>div:nth-child(3), .bse-cota-row>div:nth-child(3) {
    text-align: center;
  }

  .sp-comp .bse-cota-check, .sp-comp .bse-cota-x {
    width: 20px;
    height: 20px;
  }

  .sp-comp .bse-cota-check g {
    fill: {{ section.settings.check_color }} !important;
  }

  .sp-comp .bse-cota-x g {
    fill: {{ section.settings.x_color }} !important;
  }

  @media only screen and (max-width: 800px) {

  }
</style>

{% endcapture %} 
 {{ css | strip_newlines }}


