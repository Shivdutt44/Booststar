   .bse-reviews-section-{{ section.id }} .bse-author_placeholder{
     height:50px;
     width:50px;
   }
  .bse-reviews-section-{{ section.id }} .half-star {
              display: inline-block;
              position: relative;
              color: lightgray;
    }
    .bse-reviews-section-{{ section.id }} .half-star::before {
              content: '★';
              position: absolute;
              left: 0;
              width: 50%;
              overflow: hidden;
         color: {{section.settings.star_color}};
    }
  .bse-reviews-section-{{ section.id }} .reviews_container{
  font-size: 2rem;
  color: {{section.settings.star_color}};
  }
   .bse-reviews-section-{{ section.id }} .bse-review-wrap{
     text-align: {{ section.settings.text-align }};
     padding-inline: 10px;
   }
  .bse-reviews-section-{{ section.id }} .bse-author_image{
     display:flex;
      justify-content: {{ section.settings.text-align }};
   }
  .bse-reviews-section-{{ section.id }} .bse-review-wrap-inner{
    display:flex;
    flex-direction:column;
    padding-block: 30px;
    padding-inline: 3%;
    gap:{{ section.settings.ele-space }}px;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-wrap-style2 {
    .bse-review-stars{
      order:2;
    }
    .bse-author_image{
      order:1;
    }
    .bse-review{
      order:3;
    }
    .bse-review-author{
      order:4;
    }
    .bse-reviewer-designation{
      order:5;
    }
  }
  .bse-reviews-section-{{ section.id }} .bse-review-wrap-style3 {
    .bse-review-stars{
      order:4;
    }
    .bse-author_image{
      order:1;
    }
    .bse-review{
      order:5;
    }
    .bse-review-author{
      order:2;
    }
    .bse-reviewer-designation{
      order:3;
    }
  }
  .bse-reviews-section-{{ section.id }} .bse-review-wrap-inner{
    color: {{ section.settings.text_color }};
    background: {{ section.settings.bg_color}};
    border-radius: {{ section.settings.radius}}px;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-wrap-inner div, .bse-review p{
    margin-block: 0px;
    line-height:normal;
  }
  .bse-reviews-section-{{ section.id }} .bse-author_image_inner{
    max-width: 50px;
    max-height: 50px;
    position: relative;
    overflow: hidden;
    border-radius: 50%;
  }
  .bse-reviews-section-{{ section.id }} .bse-author_image img{
    object-fit:contain;
    width:100%;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-side-image,.bse-review-side-text{
    flex:1;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-side-image img{
    object-fit:contain;
    width:100%;
    height:auto;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-side-wrap{
    display:flex;
  }
  .bse-reviews-section-{{ section.id }}{
    margin-top: {{ section.settings.margin-top }}px;
    padding-top: {{ section.settings.padding-top}}px;
    margin-bottom: {{ section.settings.margin-bottom }}px;
    padding-bottom: {{ section.settings.padding-bottom }}px;
    padding-inline: {{ section.settings.padding-inline }}px;
  }
  .bse-reviews-section-{{ section.id }} .review_section_heading{
    text-align: {{ section.settings.text-align }};
    padding:0px;
    padding-bottom:2vw;
    margin:0px;
    /* font-size: calc(24px + 1vw);
    min-font-size: 20px; */
    max-font-size: 36px;
    color: {{ section.settings.heading_color }};
  }
  .bse-reviews-section-{{ section.id }} .splide__arrow{
    background:none;
  }
  .bse-reviews-section-{{ section.id }} .splide__arrow svg{
    fill: {{ section.settings.text_color }};
  }
  .bse-reviews-section-{{ section.id }}{
    background: {{ section.settings.section_bg_color }} url("{{ section.settings.section-image | image_url }}");
  {% if section.settings.bg_image != blank %}
    background-image: url("{{ section.settings.bg_image | image_url }}");
    background-size: {{section.settings.bg_size}};
    {% if section.settings.parallax  %}
    background-attachment: fixed;
    {% endif %}
    background-position: {{section.settings.bg_pos}};
    {% endif %}
  }
  .bse-reviews-section-{{ section.id }} .bse-reviews-wrapperr{
    max-width: {{section.settings.max_width}}px;
    margin: 0 auto;
  }
  .bse-reviews-section-{{ section.id }} .bse-review-author, .bse-reviews-section-{{ section.id }} .bse-reviewer-designation{
    margin-block: 0px !important;
    line-height:normal;
  }
   .bse-reviews-section-{{ section.id }} .bse-reviewer-designation{
     opacity:.8;
     font-style:small;
   }
</style>

  {% endcapture %} 

 {{ css | strip_newlines }}
 
  
{% if section.settings.text-align == 'center' %}
<style>
 .bse-reviews-section-{{ section.id }} .bse-author_image, .bse-reviews-section-{{ section.id }} .review_section_heading{
    margin: 0 auto;
  }
    .bse-reviews-section-{{ section.id }} .reviews_heading{
      text-align:center;
   text-decoration: underline;
    }
</style>
{% endif %}
{% if section.settings.text-align == 'left' %}
<style>
   .bse-reviews-section-{{ section.id }} .review_section_heading{
    padding-left: 40px;
  }
   .bse-reviews-section-{{ section.id }} .bse-author_image{
     padding-left: 0px;
   }
</style>
{% endif %}


{% capture html %}
{% assign short_id = section.id | slice: -10, 10 %}
  
<section class="bse-reviews-section bse-reviews-section-{{ section.id }}">
  <div class="bse-reviews-wrapperr">
  {% if section.settings.heading != blank %}
  <h2 class="review_section_heading">{{ section.settings.heading }}</h2>
    {% endif %}
  <div class="bse-reviews-wrapper bse-reviews-wrapper-{{ section.id }} splide">
    <div class="splide__track">
      <div class="splide__list">
        {% for block in section.blocks %}
          <div class="bse-review-wrap splide__slide bse-review-wrap-{{ section.settings.block-style }}">
            <div class="bse-review-wrap-inner">
              {% if section.settings.block-style == 'style4' %}

                <style>
                  .bse-reviews-section-{{ section.id }} .bse-review-wrap-style4 .bse-review-wrap-inner{
                  padding:0px;
                  }
                  .bse-reviews-section-{{ section.id }} .bse-review-wrap-style4 .bse-review-side-text {
                    display: flex;
                    flex-direction: column;
                    padding: 10px !important;
                    gap: {{ section.settings.ele-space }}px !important; 
                  }
                  
                  .bse-reviews-section-{{ section.id }} .bse-review-wrap-style4 .bse-review-wrap-inner div {
                  align-items:center;
                  }
                </style>
                <div class="bse-review-side-wrap">
                {% if block.settings.image != blank %}
                <div class="bse-review-side-image">
                   <img
                    src="{{ block.settings.image | image_url: width: block.settings.image.width }}"
                    width=""
                    height=""
                  >
                </div>
                  {% else %}
                                  <div class="bse-review-side-image">
