<script>

  function bse_review() {
  var bse_script = document.createElement('script');
  bse_script.src = "{{ 'splide.js' | asset_url }}";
  bse_script.type = 'text/javascript';

  // Ensure Splide is only initialized after the script is loaded
  bse_script.onload = function() {
    var splide = new Splide('.bse-reviews-wrapper-{{ section.id }}', {
      type: 'loop',
      perPage: {{ section.settings.sildes_to_show }},
      perMove: 1,
      pagination: {{ section.settings.show_dots }},
      autoplay: {{ section.settings.autoplay_slider }},
      breakpoints: {
        1024: {
          perPage: 3
        },
        720: {
          perPage: 1
        },
      }
    });
    splide.mount();
  };

  // Append the script to the document head
  document.head.appendChild(bse_script);
}

// Ensure the function is called after the DOM is fully loaded
document.addEventListener('DOMContentLoaded', function() {
  bse_review();
});

  bse_review();

  document.addEventListener('DOMContentLoaded', bse_review);

  if (Shopify.designMode) {
      document.addEventListener('shopify:section:unload', bse_review);
      document.addEventListener('shopify:section:load', bse_review);
  } 
</script>


