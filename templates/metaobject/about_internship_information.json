        "heading_VJtm9H": {
          "type": "heading",
          "settings": {
            "heading": "<strong>ABOUT OUR INTERNSHIP  PROGRAM</strong>",
            "heading_size": "h1"
          }
        },
        "text_BaredV": {
          "type": "text",
          "settings": {
            "text": "<p>At BOOST STAR Experts, we take pleasure in sharing our wealth of experience to inspire and propel students toward their goals. Our team, a collective of seasoned professionals with diverse streams of computer science expertise, is passionate about guiding students to make informed career decisions.</p>"
          }
        }
